def inventaire(P):
    stock = str(input()).split()
    nbOperations = int(input())

    for i in range(nbOperations):
        n, changement = map(int, input().split())
        n -= 1
        nombre = int(stock[n]) + int(changement)
        stock[n] = nombre

    for i in stock:
        reponse = stock[i], end = " "

    return reponse

P = int(input())
print(inventaire(P))

